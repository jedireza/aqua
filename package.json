  "description": "A website and user system for Node.js",
  "main": "index.js",
  "scripts": {
    "setup": "./setup.js",
    "start": "gulp",
    "test": "lab -t 100 -S -T ./node_modules/lab-babel -L --lint-options '{\"extensions\":[\".js\",\".jsx\"]}' ./test/client-before.js ./test/client/ ./test/client-after.js ./test/misc/ ./test/server/",
    "test-client": "lab -t 100 -S -T ./node_modules/lab-babel -L --lint-options '{\"extensions\":[\".js\",\".jsx\"]}' ./test/client-before.js ./test/client/ ./test/client-after.js",
    "test-server": "lab -t 100 -S -T ./node_modules/lab-babel -L --lint-options '{\"extensions\":[\".js\",\".jsx\"]}' ./test/misc/ ./test/server/",
    "test-cover": "lab -t 100 -S -T ./node_modules/lab-babel -r html -o ./test/artifacts/coverage.html ./test/client-before.js ./test/client/ ./test/client-after.js ./test/misc/ ./test/server/ && open ./test/artifacts/coverage.html"
  },
