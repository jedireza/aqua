{
  "name": "aqua",
  "version": "3.3.4",
  "description": "A website and user system for Node.js",
  "main": "index.js",
  "scripts": {
    "setup": "./setup.js",
    "start": "gulp",
    "test": "lab -t 100 -S  -L $npm_package_config_transformer_option $npm_package_config_lint_option $npm_package_config_server_tests $npm_package_config_client_tests",
    "test-client": "lab -t 100 -S $npm_package_config_transformer_option -L $npm_package_config_lint_option $npm_package_config_server_tests",
    "test-server": "lab -t 100 -S $npm_package_config_transformer_option -L $npm_package_config_lint_option' $npm_package_config_client_tests",
    "test-cover": "lab -t 100 -S $npm_package_config_transformer_option -r html -o ./test/artifacts/coverage.html $npm_package_config_client_tests $npm_package_config_server_tests && open ./test/artifacts/coverage.html",
    "coverage": "lab -s $npm_package_config_transformer_option $npm_package_config_server_tests $npm_package_config_client_tests -r lcov | coveralls",
    "update": "david update"
  },
  "author": "Reza Akhavan <jedireza@gmail.com> (http://reza.akhavan.me/)",
  "repository": "https://github.com/jedireza/aqua",
  "license": "MIT",
  "engines": {
    "node": ">=0.10.0"
  },
  "config": {
    "transformer_option": "-T ./test/transform.js",
    "lint_option": "--lint-options {\"extensions\":[\".js\",\".jsx\"]}",
    "server_tests": "./test/misc/ ./test/server/",
    "client_tests": "./test/client-before.js ./test/client/ ./test/client-after.js"
  },
  "dependencies": {
    "async": "^1.x.x",
    "bcrypt": "^0.8.x",
    "boom": "^2.x.x",
    "classnames": "^2.x.x",
    "confidence": "^1.x.x",
    "crumb": "^4.x.x",
    "glue": "^2.x.x",
    "handlebars": "^3.x.x",
    "hapi": "^8.x.x",
    "hapi-auth-basic": "^2.x.x",
    "hapi-auth-cookie": "^3.x.x",
    "hapi-mongo-models": "^2.x.x",
    "hapi-react-views": "^3.x.x",
    "hoek": "^2.x.x",
    "joi": "^6.x.x",
    "lout": "^6.x.x",
    "mongodb": "^2.x.x",
    "node-uuid": "^1.x.x",
    "nodemailer": "^1.x.x",
    "nodemailer-markdown": "^1.x.x",
    "object-assign": "^3.x.x",
    "react": "^0.13.x",
    "slug": "^0.9.x",
    "visionary": "^2.x.x"
  },
  "devDependencies": {
    "babel-core": "^5.x.x",
    "babel-loader": "^5.x.x",
    "bootstrap": "^3.x.x",
    "code": "^1.x.x",
    "cookie": "^0.1.x",
    "coveralls": "^2.11.6",
    "david": "^6.x.x",
    "del": "^1.x.x",
    "eslint-plugin-hapi": "^1.2.0",
    "flux": "^2.x.x",
    "flux-constant": "^1.x.x",
    "flux-dispatcher": "^1.x.x",
    "flux-store": "^1.x.x",
    "font-awesome": "^4.x.x",
    "gulp": "^3.x.x",
    "gulp-concat": "^2.x.x",
    "gulp-less": "^3.x.x",
    "gulp-newer": "^0.5.x",
    "gulp-nodemon": "^2.x.x",
    "gulp-rename": "^1.x.x",
    "gulp-sourcemaps": "^1.x.x",
    "gulp-util": "^3.x.x",
    "jsdom": "^3.x.x",
    "lab": "^6.2.0",
    "lodash": "^3.x.x",
    "merge-stream": "^0.1.x",
    "moment": "^2.x.x",
    "nodemon": "^1.x.x",
    "promptly": "^0.2.x",
    "proxyquire": "^1.x.x",
    "qs": "^4.x.x",
    "react-router": "^0.13.x",
    "require-dir": "^0.3.x",
    "webpack": "^1.x.x",
    "xhr": "^2.x.x"
  }
}
